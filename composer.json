{
  "name": "wazum/console-output-service",
  "type": "typo3-cms-extension",
  "description": "Provides the Symfony ConsoleOutput as a service for TYPO3 CMS",
  "license": "GPL-3.0-or-later",
  "config": {
    "allow-plugins": {
      "typo3/cms-composer-installers": true,
      "typo3/class-alias-loader": true
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "console_output_service"
    }
  },
  "autoload": {
    "psr-4": {
      "Wazum\\ConsoleOutputService\\": "Classes/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wazum\\ConsoleOutputService\\Tests\\": "Tests/"
    }
  },
  "require": {
    "php": "^8.2",
    "typo3/cms-core": "^12||^13",
    "symfony/console": "^6.0 || ^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5 || ^11.0",
    "friendsofphp/php-cs-fixer": "^3.59",
    "phpstan/phpstan": "^1.10",
    "rector/rector": "^1.0"
  }
}
